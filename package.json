{
  "name": "html5workertest",
  "version": "1.0.0",
  "description": "Test for browser features of web workers and service workers",
  "main": "index.js",
  "scripts": {
    "test-local": "concurrently --kill-others \"node bin/watch.js\" \"zuul --no-coverage --local 9000 src/index.js\"",
    "build-worker": "browserify -t babelify src/worker.js > worker-bundle.js && browserify -t babelify src/serviceWorker.js > service-worker-bundle.js",
    "check-env-vars": "node bin/check-env-vars.js",
    "standard": "standard",
    "pretest": "run-scripts standard check-env-vars build-worker",
    "test": "zuul --no-coverage src/index.js",
    "build-site": "rimraf dist && mkdirp dist && node bin/build-site.js",
    "watch-site": "rimraf dist && mkdirp dist && concurrently --kill-others \"node bin/build-site.js dev\" \"hs -p 9000\""
  },
  "keywords": [],
  "author": "Nolan Lawson <nolan@nolanlawson.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "blob-util": "^1.2.1",
    "compare-version": "^0.1.2",
    "function-to-string": "^0.2.0",
    "node-assert": "0.0.1",
    "pouchdb-http": "^5.4.5",
    "pouchdb-node": "^5.4.5",
    "pouchdb-promise": "5.4.5",
    "promise-worker": "^1.1.0",
    "ua-parser-js": "^0.7.10"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "concurrently": "^2.2.0",
    "denodeify": "^1.2.1",
    "envify": "^3.4.1",
    "handlebars": "^4.0.5",
    "http-server": "^0.9.0",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "pouchdb-http": "^5.4.5",
    "rimraf": "^2.5.3",
    "rollupify": "^0.3.4",
    "run-scripts": "^0.4.0",
    "standard": "^7.1.2",
    "watch-glob": "^0.1.3",
    "watchify": "^3.7.0",
    "zuul": "nolanlawson/zuul#post-results",
    "zuul-localtunnel": "nolanlawson/zuul-localtunnel#https"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nolanlawson/html5workertest.git"
  },
  "bugs": {
    "url": "https://github.com/nolanlawson/html5workertest/issues"
  },
  "homepage": "https://github.com/nolanlawson/html5workertest#readme"
}
